#!/bin/sh
set -e
set -x
upgradeapt

for language in ruby rust; do
  updater="update$language"
  hash "$updater" && "$updater"
done

hash dotsyncall && dotsyncall

hash upgradevimplugins && upgradevimplugins

for refreshscript in ~/.local/bin/refresh*; do
  "$refreshscript"
done

hash upgradegit && upgradegit
